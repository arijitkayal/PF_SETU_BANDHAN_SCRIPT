            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor">
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">// Set threshold
int threshold = 3000;
 
// Get response details
long responseTime = prev.getTime();
boolean isSuccess = prev.isSuccessful();
String samplerName = prev.getSampleLabel();
String responseData = prev.getResponseDataAsString().replaceAll(&quot;[\\r\\n]+&quot;, &quot; &quot;).replace(&quot;,&quot;, &quot;;&quot;); // clean response
String responseCode = prev.getResponseCode();
String url = prev.getURL().toString(); // changed from sampler.getUrl()
 
// Timestamp
String timestamp = new java.text.SimpleDateFormat(&quot;yyyy-MM-dd HH:mm:ss&quot;).format(new java.util.Date());
 
// File path (adjust as needed)
String filePath = timestamp+&quot;_api_log.csv&quot;;
 
if (responseTime &gt; threshold) {
    FileWriter fw = new FileWriter(filePath, true); // true = append mode
    PrintWriter pw = new PrintWriter(fw);
 
    // CSV Format: Timestamp,SamplerName,URL,ResponseTime(ms),Status,Code,ResponseData
    pw.println(&quot;\&quot;&quot; + timestamp + &quot;\&quot;,&quot;
             + &quot;\&quot;&quot; + samplerName + &quot;\&quot;,&quot;
             + &quot;\&quot;&quot; + url + &quot;\&quot;,&quot;
             + responseTime + &quot;,&quot;
             + &quot;\&quot;&quot; + (isSuccess ? &quot;SUCCESS&quot; : &quot;FAILURE&quot;) + &quot;\&quot;,&quot;
             + &quot;\&quot;&quot; + responseCode + &quot;\&quot;,&quot;
             + &quot;\&quot;&quot; + responseData + &quot;\&quot;&quot;);
    
    pw.close();
    fw.close();
}</stringProp>
            </BeanShellPostProcessor>
